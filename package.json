{
	"name": "@juriadams/timed-fetch",
	"description": "A tiny fetch wrapper that tracks request duration, latency, and server timings.",
	"version": "0.0.3",
	"type": "module",
	"private": false,
	"files": [
		"dist",
		"package.json",
		"readme.md",
		"changelog.md"
	],
	"module": "dist/index.mjs",
	"types": "dist/index.d.mts",
	"scripts": {
		"build": "obuild",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
		"exports": "attw --pack . --profile esm-only",
		"lint": "biome lint . && biome check .",
		"format": "biome lint --write . && biome format --write . && biome check --write .",
		"changeset": "changeset add",
		"version": "changeset version",
		"publish": "bun run build && changeset publish"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "0.18.2",
		"@biomejs/biome": "2.3.11",
		"@changesets/cli": "2.29.8",
		"@types/bun": "latest",
		"fetch-mock": "12.6.0",
		"obuild": "0.4.10"
	},
	"peerDependencies": {
		"typescript": "5.9.2"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs"
		}
	}
}
